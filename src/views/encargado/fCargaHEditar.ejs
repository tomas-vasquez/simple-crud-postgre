<%- include("../header"); %>
<div class="col-12">
    <h1>Editar <%= cargah.idcargah %></h1>
    <form method="post" action="/encargado/fCargaHActualizar">
        <input value="<%= cargah.idcargah %>" name="idcargah" type="hidden">
        <input value="<%= user%>" name="user" type="hidden">
        <div class="form-group">
            <label for="precio">Estado</label>
            <input value="<%= cargah.estado %>" required id="estado" placeholder="Estado"
                class="form-control" type="text" name="estado">
        </div>
        <div class="form-group">
            <button class="btn btn-success">Guardar</button>
            &nbsp;<a href="/encargado/fCargaH" class="btn btn-primary">Volver</a>
        </div>
    </form>
</div>
<%- include("../footer"); %>