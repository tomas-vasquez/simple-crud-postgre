<%- include("../header"); %>
<div class="col-12">
    <h1>Editar <%= docentes.nombres %> <%= docentes.apellidop %> </h1>
    <form method="post" action="/encargado/fDocenteActualizar">
        <input value="<%= docentes.codigod %>" name="codigod" type="hidden">
        <input value="<%= user%>" name="user" type="hidden">
        <div class="form-group">
            <label for="precio">Estado</label>
            <input value="<%= docentes.estado %>" required id="estado" placeholder="Estado"
                class="form-control" type="number" name="estado">
        </div>
        <div class="form-group">
            <button class="btn btn-success">Guardar</button>
            &nbsp;<a href="/encargado/fDocente" class="btn btn-primary">Volver</a>
        </div>
    </form>
</div>
<%- include("../footer"); %>